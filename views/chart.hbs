{{#section 'head'}}
    <title>聊天</title>
    <link rel="stylesheet" type="text/css" href="css/chart.css">
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/json3.min.js"></script>
{{/section}}
<div class="main">
	{{#each vacation}}
		<img src="/vacation-photo/{{imgurl}}" style="width:100px;" /><span class="name">{{name}}</span>
		<input type="hidden" value="{{imgurl}}" class="imgurl">
		<input type="hidden" value="{{myimgurl}}" class="myimgurl">
		<input type="hidden" value="{{id}}" class="id">
		<input type="hidden" value="{{myid}}" class="myid">
	{{/each}}
	<div class="testmesg"></div>
	<div class="msgout">
		<div id="message" class="message"></div>	
	</div>
	<form class="forms">
		<input type="text" name="text" class="text">
		<button class="but" type="button">点击</button>
	</form>
</div>

{{#section 'jquery'}}
	<script src="/js/chart.js" type="text/javascript"></script>
{{/section}}
